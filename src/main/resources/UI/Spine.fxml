<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!--<StackPane onDragDropped="#draged" onDragOver="#dragov" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.QYun.SuperSpineViewer.Controller.Spine">-->

<StackPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.QYun.SuperSpineViewer.Controller.Spine">
    <BorderPane fx:id="Viewer">
        <left>
               <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
               <StackPane fx:id="spinePane" minHeight="-Infinity" minWidth="-Infinity" onDragDropped="#draged" onDragOver="#dragov" prefHeight="760.0" prefWidth="360" />
               <ListView fx:id="list1" onDragDropped="#dragedL" onDragOver="#dragovL" prefHeight="200.0" prefWidth="300.0" />
            </children></VBox>

        </left>
        <center>
            <StackPane fx:id="loadPane">
                <JFXSpinner fx:id="purple" startingAngle="-40" styleClass="materialDesign-purple, first-spinner" />
                <JFXSpinner fx:id="blue" startingAngle="-90" styleClass="materialDesign-blue, second-spinner" />
                <JFXSpinner fx:id="cyan" startingAngle="-120" styleClass="materialDesign-cyan, third-spinner" />
                <JFXSpinner fx:id="green" startingAngle="-150" styleClass="materialDesign-green, fourth-spinner" />
                <JFXSpinner fx:id="yellow" startingAngle="-180" styleClass="materialDesign-yellow, fifth-spinner" />
                <JFXSpinner fx:id="orange" startingAngle="-210" styleClass="materialDesign-orange, sixth-spinner" />
                <JFXSpinner fx:id="red" startingAngle="-240" styleClass="materialDesign-red, seventh-spinner" />

            </StackPane>
        </center>
    </BorderPane>
</StackPane>
